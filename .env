# Balacify Environment Configuration
# Copy this file and rename to .env, then add your actual API keys

# API Keys - Add your actual keys here
NUTRITION_API_KEY=your_nutrition_api_key_here
GOOGLE_GENERATIVE_AI_KEY=your_google_ai_api_key_here

# Firebase Configuration (disabled temporarily)
# Uncomment and add your Firebase config when ready
# FIREBASE_API_KEY=your_firebase_api_key
# FIREBASE_PROJECT_ID=your_firebase_project_id
# FIREBASE_APP_ID=your_firebase_app_id
# FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id

# App Configuration
APP_ENV=development
APP_VERSION=2.0.0

# Debug Settings
DEBUG_MODE=true
LOG_LEVEL=debug

# Features Flags
ENABLE_FIREBASE=false
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false